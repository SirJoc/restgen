
    generator client {
      provider = "prisma-client-js"
    }
    
    datasource db {
      provider = "postgresql"
      url      = env("DATABASE_URL")
    }
    

model Workers {
  WorkerID  Int     @id
  Name      String
  Age       Int
  Gender    String
  JobID     Int
  Jobs      Jobs    @relation(fields: [JobID], references: [JobID])
}
model Jobs {
  JobID        Int       @id
  JobTitle     String
  Description  String
  Salary       Decimal
  Workers      Workers[]
}
